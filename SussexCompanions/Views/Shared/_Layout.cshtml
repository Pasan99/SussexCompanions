    <!--====== PART ENDS ======-->
    <!-- row -->
    <!--====== jquery js ======-->
    <script src="~/Content/FrontTemplate/js/vendor/modernizr-3.6.0.min.js"></script>
    <script src="~/Content/FrontTemplate/js/vendor/jquery-1.12.4.min.js"></script>

    <!--====== Bootstrap js ======-->
    <script src="~/Content/FrontTemplate/js/bootstrap.min.js"></script>
    <script src="~/Content/FrontTemplate/js/popper.min.js"></script>

    <!--====== Counter Up js ======-->
    <script src="~/Content/FrontTemplate/js/waypoints.min.js"></script>
    <script src="~/Content/FrontTemplate/js/jquery.counterup.min.js"></script>

    <!--====== Slick js ======-->
    <script src="~/Content/FrontTemplate/js/slick.min.js"></script>

    <!--====== Magnific Popup js ======-->
    <script src="~/Content/FrontTemplate/js/jquery.magnific-popup.min.js"></script>

    <!--====== Scrolling Nav js ======-->
    <script src="~/Content/FrontTemplate/js/jquery.easing.min.js"></script>
    <script src="~/Content/FrontTemplate/js/scrolling-nav.js"></script>

    <!--====== Countdown js ======-->
    <script src="~/Content/FrontTemplate/js/jquery.countdown.min.js"></script>

    <!--====== wow js ======-->
    <script src="~/Content/FrontTemplate/js/wow.min.js"></script>

    <!--====== Ajax Contact js ======-->
    <script src="~/Content/FrontTemplate/js/ajax-contact.js"></script>

    <!--====== Main js ======-->
    <script src="~/Content/FrontTemplate/js/main.js"></script>

</body>

</html>
